{% macro addSetGet(property) %}

    /**
{% if property.description %}
     * {{ property.description }}
     *
{% endif %}
{% if property.deprecated %}
     * @deprecated {{ property.deprecated }}
     *
{% endif %}
     * @return {{ property.type }}{% if not property.required %}|null{% endif %}

     */
    public function get{{ property.actionName }}(): {% if not property.required %}?{% endif %}{{ property.type }}
    {
        return $this->{{ property.name }};
    }

    /**
{% if property.description %}
     * {{ property.description }}
     *
{% endif %}
     * @param {{ property.type }}{% if not property.required %}|null{% endif %} ${{property.name}}
     *
{% if property.deprecated %}
     * @deprecated {{ property.deprecated }}
     *
{% endif %}
     * @return self
     */
    public function set{{ property.actionName }}({% if not property.required %}?{% endif -%}{{ property.type }} ${{ property.name }}): self
    {
        $this->{{property.name}} = ${{property.name}};

        return $this;
    }
{% endmacro %}